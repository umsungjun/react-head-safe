{
  "name": "react-head-safe",
  "version": "1.0.0",
  "description": "A CSR-only React head manager that prevents duplicate meta tags.",
  "author": "",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "react": ">=17"
  },
  "devDependencies": {
    "@types/react": "^19.2.7",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.5.2",
  "keywords": ["react", "seo", "meta", "head", "csr"]
}
